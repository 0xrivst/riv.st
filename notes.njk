---
subtitle: notes
layout: layouts/root.njk
---

<header>
    <h1>notes
    </h1>
</header>
<hr/>
<main>
    <section>
        <ul>
            {% for note in collections.note %}
                <li>
                    <a href="{{ note.url }}">{{ note.data.title }}</a>
                    - {{ note.date | post_date }}</li>
            {% endfor %}
        </ul>
    </section>
</main>